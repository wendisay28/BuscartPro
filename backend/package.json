{
  "name": "buscartpro-backend",
  "version": "1.0.0",
  "description": "Backend for BuscartPro application",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "db:init": "NODE_OPTIONS='--loader ts-node/esm' scripts/init-db.ts",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "migrate:custom": "NODE_OPTIONS='--loader ts-node/esm' scripts/migrate.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.18",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.12",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.43.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.4.0",
    "fs-extra": "^11.3.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "pg": "^8.16.0",
    "postgres": "^3.4.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.22",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.15.29",
    "@types/pg": "^8.15.2",
    "drizzle-kit": "^0.31.1",
    "mysql2": "^3.14.1",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
